{
  "schema": {
    "type": "object",
    "properties": {
      "name": {
        "type": "string",
        "description": "Name of your bundle",
        "pattern": "\b[a-z0-9]+(?:_[a-z0-9]+)*\b"
      },
      "tabs": {
        "description": "Tabs that will be loaded with this bundle",
        "type": "array",
        "items": {
          "type": "string"
        }
      }
    },
    "required": ["name"]
  }
}