{
  "name": "@sourceacademy/modules-lib",
  "description": "Common library used by Source Academy bundles and tabs",
  "private": true,
  "type": "module",
  "devDependencies": {
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "eslint": "^9.29.0",
    "typedoc": "^0.28.4",
    "typedoc-plugin-frontmatter": "^1.3.0",
    "typedoc-plugin-markdown": "^4.7.0",
    "typedoc-plugin-rename-defaults": "^0.7.3",
    "typescript": "^5.8.2",
    "vitest": "^3.2.3"
  },
  "exports": {
    "./*": "./dist/*.js",
    "./tabs": "./dist/tabs/index.js",
    "./types": "./dist/types/index.js"
  },
  "dependencies": {
    "@blueprintjs/core": "^5.10.2",
    "@blueprintjs/icons": "^5.9.0",
    "js-slang": "^1.0.81",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "scripts-info": {
    "docs": "Build the documentation for documentation server using Typedoc"
  },
  "scripts": {
    "build": "tsc --project ./tsconfig.prod.json",
    "docs": "typedoc",
    "lint": "eslint src",
    "prepare": "yarn build",
    "tsc": "tsc --project ./tsconfig.json",
    "test": "vitest --project \"Modules Library\""
  }
}
