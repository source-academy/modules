{
  "private": true,
  "name": "modules",
  "version": "1.0.0",
  "repository": "https://github.com/source-academy/modules.git",
  "license": "Apache-2.0",
  "scripts": {
    "module": "node ./templates/app.js",
    "lint": "./node_modules/.bin/eslint --config ./.eslintrc.js --ext \".ts, .tsx\" src",
