{
  "name": "@sourceacademy/bundle-robot_simulation",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@sourceacademy/modules-lib": "workspace:^",
    "js-slang": "^1.0.85",
    "three": "^0.181.0"
  },
  "devDependencies": {
    "@dimforge/rapier3d-compat": "^0.11.2",
    "@sourceacademy/modules-buildtools": "workspace:^",
    "@types/three": "^0.181.0",
    "typescript": "^5.8.2"
  },
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*.js",
    "./controllers": "./dist/controllers/index.js",
